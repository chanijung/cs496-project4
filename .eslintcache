sole.log(getHasToken());\n                setHasToken(response.token);\n            } else {\n                throw new Error(response.error);\n            }\n        } catch (err) {\n            alert('로그인에 실패했습니다.');\n            setUserId('');\n            setUserPw('');\n            console.error('login error', err);\n        }\n    };\n    return (\n        <div className=\"loj2\">\n            <h2 className=\"login\">로그인</h2>\n\n            <form className=\"login-form\"\n                onSubmit={handleSubmit}\n            >\n                <div className=\"idpwd\">\n                    <input className=\"idpwd_input\"\n                        type=\"text\"\n                        name=\"uid\"\n                        value={userId}\n                        onChange={e => setUserId(e.target.value)}\n                        placeholder=\"아이디\"\n                    />\n                </div>\n                <br/>\n                <div className=\"idpwd\">\n                    <input className=\"idpwd_input\"\n                        type=\"password\"\n                        name=\"pwd\"\n                        value={userPw}\n                        onChange={e => setUserPw(e.target.value)}\n                        placeholder=\"비밀번호\"\n                    />\n                </div>\n                <br/>\n                <button className=\"login_button\" type=\"submit\">\n                    로그인\n                </button>\n            </form>\n            <div className=\"join-button\">\n                <Link to=\"/join\">\n                    회원가입\n                </Link>\n            </div>\n        </div>\n    );\n};\nexport default Login;\n","/home/ubuntu/project4 copy/project4/src/components/second_tab/famehall.js",["197","198","199","200","201","202","203"],"import React, { Component, useState } from \"react\";\nimport { Route, Switch, Redirect } from \"react-router-dom\";\nimport {BrowserRouter as Router,Link} from \"react-router-dom\";\nimport Fa